{
  "name": "rest-api-boilerplate",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "now": {
    "name": "rest-api-boilerplate",
    "alias": ["rest-api-boilerplate"],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "scripts": {
    "dev": "nodemon server/server.js --watch common --watch server",
    "lint": "eslint .",
    "start": "node .",
    "now:deploy": "now",
    "now:alias": "now alias",
    "now": "npm run now:deploy && npm run now:alias"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "loopback-connector-mongodb": "^3.3.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^3.17.1",
    "eslint-config-loopback": "^8.0.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-prettier": "^2.3.1",
    "nodemon": "^1.12.1",
    "nsp": "^2.1.0",
    "prettier": "^1.8.1"
  },
  "repository": {
    "type": "",
    "url": ""
